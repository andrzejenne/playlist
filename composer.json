{
    "name": "bbit/playlist",
    "description": "The caching playlist.",
    "keywords": ["playlist", "youtube", "cache"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "alaouy/youtube": "^2.1",
        "barryvdh/laravel-cors": "^0.11.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~4.0",
        "intervention/image": "^2.4",
        "james-heinrich/getid3": "^1.9",
        "jolitagrazyte/laravel-discogs": "dev-master",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^5.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "pelim/laravel-zmq": "^1.0",
        "symfony/process": "^4.0",
        "thruway/pawl-transport": "^0.5.0",
        "vimeo/vimeo-api": "^2.0",
        "voryx/thruway": "^0.5.2"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nikic/php-parser": "^3.1.5",
        "nunomaduro/collision": "~2.0",
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0",
        "weebly/phpstan-laravel": "^1.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/andrzejenne/laravel-ratchet"
        }
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "BBIT\\Playlist\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "stan": "phpstan analyse app --level 7 -c tests/phpstan.neon"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
